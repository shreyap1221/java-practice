package test_project;  //created a package named test_project inside TestProject project in eclipse IDLE

public class TestProject {   //created a public class  named TestProject same as project name by convention

	public static void main(String[] args) {  //main function-- execution of program starts here
		// TODO Auto-generated method stub
		//Method overloading problem
		//functions with same name but different parameters and return types
		int l=20;
		int b=5;
		float r=1.5f;
		int rectangle_area = area(l,b);
		float circle_area = area(r);
		
		System.out.println("Area of rectangle is "+ rectangle_area);
		System.out.println("Area of circle is "+ circle_area);
	
	}
	
	public static int area(int n1,int n2) {
		int result= n1*n2;
		
		return result;
		
	}
	public static float area(float a) {
		float result= 3.14f*a*a;
		
		return result;
		
	}
}
