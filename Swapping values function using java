package test_project;  //created a package named test_project inside TestProject project in eclipse IDLE

public class TestProject {   //created a public class  named TestProject same as project name by convention

	public static void main(String[] args) {  //main function-- execution of program starts here
		// TODO Auto-generated method stub
		
		int a =10,b=90;
		swapFunction(a,b);
 
	}
	public static void swapFunction(int num1,int num2) {
		
		System.out.println("The values before swapping are a: " + num1 + "; b: " +num2);
		int c;
		c=num1;
		num1=num2;
		num2=c;
		System.out.println("The values after swapping are a: " + num1 + "; b: " +num2);
	}
}
