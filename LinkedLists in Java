package test_project;  //created a package named test_project inside TestProject project in eclipse IDLE

import java.util.LinkedList;
import java.util.Scanner;
public class TestProject {   //created a public class  named TestProject same as project name by convention

	public static void main(String[] args) {  //main function-- execution of program starts here
		// TODO Auto-generated method stub
		//LinkedLists in Java
		LinkedList<String> names = new LinkedList<String>();
		
		names.add("Shreya");
		names.add("Atharva");
		names.add("Shruti");
		names.add("Sukanya");
		names.add("Prachi");
		System.out.println(names); //output: [Shreya, Atharva, Shruti, Sukanya, Prachi]
		names.addFirst("Shweta");   //adds element at beginning of list
		System.out.println(names); //output: [Shweta, Shreya, Atharva, Shruti, Sukanya, Prachi]
		names.addLast("Swati");     //adds element from end of list
		System.out.println(names); //output: [Shweta, Shreya, Atharva, Shruti, Sukanya, Prachi, Swati]
		names.removeFirst();       //removes element at beginning of list
		System.out.println(names); //output: [Shreya, Atharva, Shruti, Sukanya, Prachi, Swati]
		names.removeLast();        //removes element from end of list
		System.out.println(names); //output: [Shreya, Atharva, Shruti, Sukanya, Prachi]
		
	}
}
